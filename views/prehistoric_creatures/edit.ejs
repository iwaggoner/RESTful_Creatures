
<form action="/prehistoric_creatures/<%= creatureId %>/?_method=PUT" method="POST">
    <label for="creatureURL">Image URL</label>
    <input type="text" id="img_url" name="img_url" value="<%=creature.img_url%>">

    <label for="Type">Type</label>
    <input type="text" name="type" id="type" value="<%=creature.type%>">

    <input type="submit" value="Update Creature">

</form>